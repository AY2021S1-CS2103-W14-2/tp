@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "c:CheckInCommand" as CheckInCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":BookingBook" as BookingBook MODEL_COLOR
end box
[-> LogicManager : execute(checkIn)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(checkIn)
activate AddressBookParser

create CheckInCommand
AddressBookParser -> CheckInCommand
activate CheckInCommand

CheckInCommand --> AddressBookParser
deactivate CheckInCommand

AddressBookParser --> LogicManager : c
deactivate AddressBookParser

LogicManager -> CheckInCommand : execute()
activate CheckInCommand

CheckInCommand -> Model : addBooking()
activate Model

Model -> BookingBook : addBooking()
activate BookingBook

BookingBook --> Model :
deactivate BookingBook

Model --> CheckInCommand
deactivate Model

CheckInCommand --> LogicManager : result
deactivate CheckInCommand
CheckInCommand -[hidden]-> LogicManager : result
destroy CheckInCommand

[<--LogicManager
deactivate LogicManager
@enduml
